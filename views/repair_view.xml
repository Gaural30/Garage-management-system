<?xml version="1.0" encoding="utf-8" ?>



<!-- Not Visibe on Website  -->



<odoo>

<record id="view_repair_form" model="ir.ui.view">
        <field name="name">view_repair_form</field>
        <field name="model">garage.repair</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

<record id="view_repair_tree" model="ir.ui.view">
    <field name="name">view_repair_tree</field>
    <field name="model">garage.repair</field>
    <field name="arch" type="xml">
        <list>
            <field name="name"/>
        </list>
    </field>
</record>

<record id="view_repair_search" model="ir.ui.view">
    <field name="name">view_repair_search</field>
    <field name="model">garage.repair</field>
    <field name="arch" type="xml">
        <search>
            <field name="name"/>
        </search>
    </field>
</record>

<record id="action_garage_repair" model="ir.actions.act_window">
        <field name="name">Repair</field>
        <field name="res_model">garage.repair</field>
        <field name="view_mode">list,form</field>
</record>
        
